{
    "name": "ilove2caremobile",
    "version": "0.0.1",
    "private": true,
    "description": "I love to care mobile app",
    "scripts": {
        "start": "ionic serve -p 3000",
        "server": "nodemon --watch server server/index.js",
        "serve:build": "npx serve -s build",
        "build:android": "npx cap add android && npx cap copy android && cd android && ./gradlew assembleDebug && cd ..",
        "sync": "npm build && npx cap sync",
        "dev:ios": "ionic capacitor run ios -l --external",
        "dev:and": "ionic capacitor run android -l --external",
        "open:and": "npx cap open android",
        "open:ios": "npx cap open ios",
        "build": "react-scripts build",
        "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
        "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "npm run lint -- --fix",
        "typecheck": "tsc --noEmit --incremental false",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "@capacitor/cli": "2.4.2",
        "@types/jest": "^26.0.15",
        "@types/mobx-task": "^2.0.1",
        "@types/react": "^16.9.56",
        "@types/react-dom": "^16.9.9",
        "@types/react-router": "^5.1.8",
        "@types/react-router-dom": "^5.1.6",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "eslint": "^7.11.0",
        "eslint-config-airbnb-typescript": "^12.0.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-import-helpers": "^1.1.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "husky": "^4.3.0",
        "lint-staged": "^10.5.1",
        "prettier": "^2.1.2"
    },
    "dependencies": {
        "@capacitor/android": "2.4.2",
        "@capacitor/core": "2.4.2",
        "@capacitor/ios": "2.4.2",
        "@ionic/react": "^5.4.2",
        "@ionic/react-router": "^5.4.2",
        "@testing-library/jest-dom": "^5.11.5",
        "@types/node": "^14.14.6",
        "axios": "^0.21.0",
        "cordova-plugin-ionic": "5.4.7",
        "cordova-plugin-whitelist": "^1.3.4",
        "firebase": "^8.0.1",
        "firebaseui": "^4.7.1",
        "ionicons": "^5.2.3",
        "mobx": "^5.15.5",
        "mobx-react-lite": "^2.0.7",
        "mobx-task": "^2.0.1",
        "node-sass": "^4.14.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-firebaseui": "^4.1.0",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.0",
        "typescript": "4.0.5"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint:fix"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}

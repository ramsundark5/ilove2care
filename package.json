{
    "name": "ilove2caremobile",
    "version": "1.0.0",
    "private": true,
    "description": "I love to care mobile app",
    "scripts": {
        "start": "npm run lint:fix && ionic serve -p 3000",
        "server": "nodemon --watch server server/index.js",
        "serve:build": "npx serve -s build",
        "build:android": "npx cap add android && npx cap copy android && cd android && ./gradlew assembleDebug && cd ..",
        "sync": "npm build && npx cap sync",
        "dev:ios": "ionic capacitor run ios -l --external",
        "dev:and": "ionic capacitor run android -l --external",
        "open:and": "npx cap open android",
        "open:ios": "npx cap open ios",
        "build": "react-scripts build",
        "prod": "npx cap copy android && cordova-res android --skip-config --copy && ionic capacitor build android",
        "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
        "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "npm run lint -- --fix",
        "typecheck": "tsc --noEmit --incremental false",
        "cypress:open": "cypress open",
        "eject": "react-scripts eject",
        "codepush": "npm run build && appcenter codepush release -a ilove2caremobile-gmail.com/llovetocare -c android/app/src/main/assets/public/ --target-binary-version \"1.0\" -d Production"
    },
    "devDependencies": {
        "@capacitor-community/http": "^1.4.1",
        "@capacitor/cli": "^3.3.2",
        "@capacitor/device": "^1.1.0",
        "@capacitor/dialog": "^1.0.6",
        "@capacitor/filesystem": "^1.0.6",
        "@ionic/cli": "^6.18.1",
        "@types/jest": "^26.0.24",
        "@types/mobx-task": "^2.0.1",
        "@types/react": "^16.14.21",
        "@types/react-dom": "^16.9.14",
        "@types/react-geosuggest": "^2.7.13",
        "@types/react-router": "^5.1.17",
        "@types/react-router-dom": "^5.3.2",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "capacitor-codepush": "git+https://github.com/mapiacompany/capacitor-codepush.git",
        "cypress": "^6.9.1",
        "cypress-movie": "^3.0.19",
        "cypress-react-selector": "^2.3.13",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-import-helpers": "^1.2.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "husky": "^4.3.8",
        "lint-staged": "^10.5.4",
        "prettier": "^2.4.1"
    },
    "dependencies": {
        "@capacitor/android": "^3.3.2",
        "@capacitor/core": "^3.3.2",
        "@capacitor/ios": "^3.3.2",
        "@fullcalendar/interaction": "^5.10.1",
        "@fullcalendar/list": "^5.10.1",
        "@fullcalendar/react": "^5.10.1",
        "@hookform/resolvers": "^1.3.7",
        "@ionic/react": "^5.9.1",
        "@ionic/react-router": "^5.9.1",
        "@testing-library/jest-dom": "^5.15.0",
        "@types/lodash": "^4.14.177",
        "@types/node": "^14.17.34",
        "@types/uuid": "^8.3.3",
        "@types/yup": "^0.29.13",
        "axios": "^0.21.4",
        "dayjs": "^1.10.7",
        "firebase": "^8.10.0",
        "firebaseui": "^4.8.1",
        "ionicons": "^5.5.4",
        "lodash": "^4.17.21",
        "loglevel": "^1.8.0",
        "logrocket": "^1.3.0",
        "mobx": "^6.3.7",
        "mobx-react-lite": "^3.2.2",
        "mobx-task": "^2.0.1",
        "node-sass": "^4.14.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-firebaseui": "^4.2.0",
        "react-geosuggest": "^2.14.1",
        "react-hook-form": "^6.15.8",
        "react-phone-input-2": "^2.14.0",
        "react-router": "^5.2.1",
        "react-router-dom": "^5.3.0",
        "react-scripts": "4.0.0",
        "typescript": "4.0.5",
        "uuid": "^8.3.2",
        "yup": "^0.30.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint:fix"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "minipass": "2.7.0"
    }
}
